[project]
	description =  | Repo Category: custom, History Protection: On
[access]
	inheritFrom = iwlwifi-All-Projects
[receive]
	requireChangeId = true
	requireSignedOffBy = true
[submit]
	action = rebase always
	mergeContent = true
[access "refs/meta/config"]
	read = group Administrators
	push = +force group Administrators
	submit = group Administrators
	forgeAuthor = group Administrators
	forgeCommitter = group Administrators
	exclusiveGroupPermissions = push pushMerge submit
[access "^refs/(heads|tags)/[0-9a-fA-F]{40}"]
	create = block group Anonymous Users
	createTag = block group Anonymous Users
[access "^refs/tags/origin/.*"]
	create = block group Anonymous Users
	createTag = block group Anonymous Users
[access "refs/*"]
	owner = group Administrators
	read = group Registered Users
	abandon = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	abandon = group iwlwifi-automation
	create = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	push = +force group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	pushMerge = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	label-Code-Review = -2..+2 group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	label-Code-Review = -1..+1 group Registered Users
	label-Verified = -1..+1 group Registered Users
	rebase = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	removeReviewer = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	submit = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	editTopicName = +force group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	viewDrafts = group iwlwifi-automation
	addPatchSet = group Registered Users
	forgeAuthor = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	forgeCommitter = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	editAssignee = group Registered Users
	label-automatic-review = -1..+1 group iwlwifi-automation
[access "refs/for/refs/heads/*"]
	push = group Registered Users
[access "refs/release/mvt/*"]
	push = +force group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	pushMerge = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	submit = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
[access "refs/drafts/*"]
	push = group Registered Users
[access "refs/heads/common/*"]
	push = +force group Registered Users
	pushMerge = group Registered Users
	label-Code-Review = -2..+2 group Registered Users
	submit = group Registered Users
	create = group Registered Users
	forgeAuthor = group Registered Users

[access "refs/heads/sandbox/${username}/*"]
	create = group Registered Users
	push = +force group Registered Users
	pushMerge = group Registered Users
	label-Code-Review = -2..+2 group Registered Users
	submit = group Registered Users
	forgeAuthor = group Registered Users
	forgeCommitter = group Registered Users
	forgeServerAsCommitter = group Registered Users
[access "refs/tags/*"]
	create = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	create = group iwlwifi-automation
	forgeCommitter = group iwlwifi-automation
	forgeAuthor = group iwlwifi-automation
	push = +force group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	push = +force group iwlwifi-automation
	pushMerge = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	pushMerge = group iwlwifi-automation
	createTag = +force group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	createTag = +force group iwlwifi-automation
	createSignedTag = group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	createSignedTag = group iwlwifi-automation
[access "refs/tags/common/*"]
	create = group Registered Users
	push = group Registered Users
	push = +force group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	push = +force group iwlwifi-automation
	createTag = group Registered Users
	createTag = +force group ldap/WCS_GERRIT_MAINT_LINUX_DRIVER
	createTag = +force group iwlwifi-automation
[commentlink "jira"]
	match = [Jj][Ii][Rr][Aa]:([A-Z]+-\\d+)
	link = https://jira.idoc.intel.com/browse/$1
[commentlink "fixes"]
	match = fixes=(I[A-Fa-f0-9]+)
	link = "/gerrit/#/q/$1"
[commentlink "cros"]
	match = chrome-os-partner:(\\d+)
	link = https://partnerissuetracker.corp.google.com/issues/$1
[commentlink "korg"]
	match = kernel.org:(\\d+)
	link = https://bugzilla.kernel.org/show_bug.cgi?id=$1
[commentlink "chromium"]
	match = chromium.org:(\\d+)
	link = https://bugs.chromium.org/p/chromium/issues/detail?id=$1
[label "automatic-review"]
	function = NoBlock
	value = -1 Some things not OK
	value =  0 not checked
	value = +1 Everything OK
	defaultValue = 0
[label "Code-Review"]
	function = MaxWithBlock
	value = -2 Do not submit
	value = -1 I would prefer that you didn't submit this
	value =  0 No score
	value = +1 Looks good to me, but someone else must approve
	value = +2 Looks good to me, approved
	copyAllScoresOnTrivialRebase = true
	defaultValue = 0
