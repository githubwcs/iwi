[project]
	description = [RepoCategory:unmanaged] | Repo Category: unmanaged
[access]
	inheritFrom = TF-Projects
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[submit]
	action = cherry pick
[access "refs/meta/config"]
	read = group TF: iwlwifi: tf-admin
	push = +force group TF: iwlwifi: tf-admin
	submit = group TF: iwlwifi: tf-admin
	forgeAuthor = group TF: iwlwifi: tf-admin
	forgeCommitter = group TF: iwlwifi: tf-admin
	exclusiveGroupPermissions = push pushMerge submit
[access "refs/*"]
	owner = group TF: iwlwifi: maint-iw
	read = group Registered Users
	abandon = group TF: iwlwifi: maint-iw
	create = group TF: iwlwifi: maint-iw
	forgeAuthor = group TF: iwlwifi: maint-iw
	forgeCommitter = group TF: iwlwifi: maint-iw
	push = +force group TF: iwlwifi: maint-iw
	pushMerge = group TF: iwlwifi: maint-iw
	label-Code-Review = -2..+2 group TF: iwlwifi: maint-iw
	label-Code-Review = -1..+1 group Registered Users
	label-Verified = -1..+1 group Registered Users
	rebase = group TF: iwlwifi: maint-iw
	removeReviewer = group TF: iwlwifi: maint-iw
	submit = group TF: iwlwifi: maint-iw
	editTopicName = +force group TF: iwlwifi: maint-iw
	forgeServerAsCommitter = group TF: iwlwifi: maint-iw
[access "refs/for/refs/heads/*"]
	push = group Registered Users
[access "refs/drafts/*"]
	push = group Registered Users
[access "refs/heads/common/*"]
	forgeAuthor = group Registered Users
	forgeCommitter = group Registered Users
	push = +force group Registered Users
	pushMerge = group Registered Users
	label-Code-Review = -2..+2 group Registered Users
	submit = group Registered Users
[access "refs/heads/sandbox/${username}/*"]
	create = group Registered Users
	forgeAuthor = group Registered Users
	forgeCommitter = group Registered Users
	push = +force group Registered Users
	pushMerge = group Registered Users
	label-Code-Review = -2..+2 group Registered Users
	submit = group Registered Users
[access "refs/tags/*"]
	create = group TF: iwlwifi: maint-iw
	create = group TF: iwlwifi: taggers
	forgeCommitter = group TF: iwlwifi: maint-iw
	forgeCommitter = group TF: iwlwifi: taggers
	forgeAuthor = group TF: iwlwifi: maint-iw
	forgeAuthor = group TF: iwlwifi: taggers
	push = +force group TF: iwlwifi: maint-iw
	push = +force group TF: iwlwifi: taggers
	pushMerge = group TF: iwlwifi: maint-iw
	pushMerge = group TF: iwlwifi: taggers
	pushTag = +force group TF: iwlwifi: maint-iw
	pushTag = +force group TF: iwlwifi: taggers
	pushSignedTag = group TF: iwlwifi: maint-iw
	pushSignedTag = group TF: iwlwifi: taggers
	forgeServerAsCommitter = group TF: iwlwifi: maint-iw

