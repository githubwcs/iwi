[project]
	description = [RepoCategory:custom] | Repo Category: custom, History Protection: On
[access]
	inheritFrom = TF-Projects/proj5201
[receive]
	requireChangeId = true
	requireSignedOffBy = true
[submit]
	action = cherry pick
	mergeContent = true
[access "refs/meta/config"]
	read = group TF: iwlwifi: tf-admin
	push = +force group TF: iwlwifi: tf-admin
	submit = group TF: iwlwifi: tf-admin
	forgeAuthor = group TF: iwlwifi: tf-admin
	forgeCommitter = group TF: iwlwifi: tf-admin
	exclusiveGroupPermissions = submit
[access "refs/*"]
	owner = group TF: iwlwifi: tf-admin
	read = group TF: Project Member of proj5201 (iwlwifi)
	read = group TF: iwlwifi: tf-admin
	abandon = group TF: iwlwifi: maint-iw
	abandon = group TF: iwlwifi: tf-admin
