[commentlink "jira"]
	match = [Jj][Ii][Rr][Aa]:([A-Z]+-\\d+)
	link = https://jira01.devtools.intel.com/browse/$1
[commentlink "fixes"]
	match = fixes=(I[A-Fa-f0-9]+)
	link = "https://git-amr-3.devtools.intel.com/gerrit/#/q/$1"
[commentlink "cros"]
	match = chrome-os-partner:(\\d+)
	link = https://partnerissuetracker.corp.google.com/issues/$1
[commentlink "korg"]
	match = kernel.org:(\\d+)
	link = https://bugzilla.kernel.org/show_bug.cgi?id=$1
[label "automatic-review"]
	function = NoBlock
	value = -1 Some things not OK
	value =  0 not checked
	value = +1 Everything OK
	defaultValue = 0
[access "refs/meta/dashboards/iwlwifi(proj5201)"]
	read = deny group TF: Project Member of proj5201 (iwlwifi)
